<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.ts.bean">
 <class name="TempData">
  <id column="id" name="id" type="java.lang.Integer">
   <generator class="identity"/>
  </id>
  <property column="date" generated="never" lazy="false"
    name="date" not-null="true" type="java.util.Date" unique="false"></property>
  
  <property column="temperature" generated="never" lazy="false"
    name="temperature" not-null="true" type="java.lang.Double" unique="false"></property> 
    
  <property column="runTime" generated="never" lazy="false"
    name="runTime" not-null="true" type="int" unique="false"></property> 
    
 </class>
</hibernate-mapping>
